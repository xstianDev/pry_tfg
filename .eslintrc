{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "warn",
        "no-undef": "error",
        "no-unused-vars": "warn",
        "no-unused-expressions": "warn"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
